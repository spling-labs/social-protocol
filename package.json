{
  "name": "@spling/social-protocol",
  "version": "1.1.5",
  "description": "The official spling social protocol. Create your own solona on-chain social media app with this protocol.",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest --forceExit",
    "clean": "rimraf dist",
    "prebuild": "yarn clean",
    "prepublishOnly": "yarn build",
    "docs": "typedoc --excludePrivate --includeVersion --out ./docs src",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx,json}",
    "lint:fix": "eslint --fix src/**/*.{js,jsx,ts,tsx,json}",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,md,json} --config ./.prettierrc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/spling-protocol/social.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "solana",
    "blockchain",
    "protocol",
    "spling"
  ],
  "author": "Spling",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spling-protocol/social/issues"
  },
  "homepage": "https://spling.app/",
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "jest-config": "^28.1.3",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.7",
    "ts-jest-resolver": "^2.0.0",
    "typedoc": "^0.23.8",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@project-serum/anchor": "^0.25.0",
    "@shadow-drive/sdk": "^3.0.5",
    "bs58": "^5.0.0",
    "dayjs": "^1.11.4"
  }
}
